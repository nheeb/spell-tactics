; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="SpellTactics"
run/main_scene="res://Logic/Main.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
run/max_fps=144
config/icon="res://Assets/Sprites/Icons/active-button/02_border.png"

[autoload]

Game="*res://Logic/AutoLoad/Game.gd"
Utility="*res://Logic/GeneralUtility/Utility.gd"
Events="*res://Logic/AutoLoad/Events.gd"
VisualTime="*res://Logic/AutoLoad/VisualTime.gd"
DebugInfo="*res://Logic/AutoLoad/DebugInfo.gd"
ActivityManager="*res://Logic/Activities/ActivityManager.gd"
Adventure="*res://Logic/Adventure/Adventure.gd"
VFX="*res://Logic/AutoLoad/VFX.tscn"
Sx="*res://addons/signal_extensions/sx_autoload.gd"

[debug]

gdscript/warnings/unused_variable=0
gdscript/warnings/unused_parameter=0
gdscript/warnings/unused_signal=0
gdscript/warnings/redundant_await=0

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
mouse_cursor/custom_image="res://Assets/Sprites/Cursor/wood_oak_24.png"

[dotnet]

project/assembly_name="SpellTactics"

[editor]

script/templates_search_path="res://Logic/ScriptTemplates"
naming/scene_name_casing=0

[editor_plugins]

enabled=PackedStringArray("res://addons/signal_extensions/plugin.cfg", "res://addons/smoothing/plugin.cfg")

[file_customization]

folder_colors={
"res://Assets/": "orange",
"res://Entities/": "purple",
"res://Levels/": "red",
"res://Logic/": "blue",
"res://Shaders/": "pink",
"res://Spells/": "teal",
"res://UI/": "yellow"
}

